geom_density_ridges2(scale = 3, jittered_points = TRUE, position = position_points_jitter(width = 0.05, height = 0), point_shape = "|", point_size = 1, alpha = 0.8) +
scale_x_continuous(labels=scales::dollar_format()) +
coord_cartesian(clip = "off") +
theme_void() +
theme(text = element_text(size = 8, family = "IBM Plex Mono", color = "#000000"),
plot.title = element_text(family = "IBM Plex Mono", size = 24, hjust = 0.5, face = "bold"),
plot.title.position = "plot",
plot.subtitle = element_markdown(family = "IBM Plex Mono", hjust = 0.5),
plot.caption = element_text(hjust = 0.5),
axis.title.x = element_text(size = 8, family = "IBM Plex Mono", color = "#000000"),
axis.text = element_text(size = 8, family = "IBM Plex Mono", color = "#000000"),
axis.title.y = element_blank(),
legend.position = "Null",
plot.margin = unit(c(1, 1, 1, 1), "cm"),
plot.background = element_rect(color = "#f2f2f2", fill = "#f2f2f2")) +
labs(title = "San Francisco",
subtitle = "<span style = 'font-size:10pt;'>Rental Prices are Increasing<br></span>",
caption = "\nData: Kate Pennington's Bay Area Craigslist Rental Housing Posts\n\n#TidyTuesday | Design: Ryan Hart",
x = "\nRental Price (USD)")
# save plot
ggsave(paste0("SF_Rents_", format(Sys.time(), "%d%m%Y"), ".png"), dpi = 320, width = 6, height = 6)
